{
  "form": "#searchForm",
  "field": "input[name='search']",
  "select": {
    "selector": "select[name='critere']",
    "options": [
      {
        "key": "activites/codePostal",
        "value": "Code Postal"
      },{
        "key": "activites/commune",
        "value": "Commune"
      },{
        "key": "activites/activites",
        "value": "Activite"
      }
    ]
  },
  "url": {
    "regex": "/%route%/%value%",
    "replaceValue": "%value%",
    "replaceRoute": "%route%"
  }
}